{
  "name": "reduxapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run cpy && NODE_ENV=production webpack -p --config webpack.config.js",
    "cpy:style": "cp -rf ./public/stylesheets ./dist/stylesheets",
    "precpy": "rm -rf dist",
    "cpy": "mkdir ./dist && cp -rf ./public/images ./dist/images && npm run cpy:style",
    "dev:client": "npm run cpy && webpack -w",
    "dev:server": "nodemon bin/www & nodemon apiServer.js",
    "start": "npm run build && node bin/www & node apiServer.js"
  },
  "engines": {
    "node": "v8.0.0"
  },
  "devDependencies": {
    "redux-logger": "3.0.6"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.16.0",
    "body-parser": "1.17.2",
    "connect-mongo": "1.3.2",
    "cookie-parser": "1.4.3",
    "debug": "~2.6.3",
    "ejs": "2.5.6",
    "express": "~4.15.2",
    "express-session": "1.15.4",
    "http-proxy": "1.16.2",
    "mongoose": "4.11.1",
    "morgan": "1.8.2",
    "react": "15.5.4",
    "react-bootstrap": "0.31.0",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "serve-favicon": "2.4.3",
    "webpack": "2.6.1"
  },
  "keywords": [],
  "author": "Anoop Mundathan <anoop.mundathan@gmail.com> (http://anoopmundathan.me/)",
  "license": "ISC"
}
